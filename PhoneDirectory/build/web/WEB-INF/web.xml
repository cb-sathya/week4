        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DataBase</servlet-name>
        <url-pattern>/DataBase</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dataBase</servlet-name>
        <url-pattern>/dataBase</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
