    <servlet-mapping>
        <servlet-name>Directory</servlet-name>
        <url-pattern>/Directory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deactivate</servlet-name>
        <url-pattern>/deactivate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>nameMatch</servlet-name>
        <url-pattern>/nameMatch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editContact</servlet-name>
        <url-pattern>/editContact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>partialMatch</servlet-name>
        <url-pattern>/partialMatch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>numberMatch</servlet-name>
        <url-pattern>/numberMatch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addContacts</servlet-name>
        <url-pattern>/addContacts</url-pattern>
    </servlet-mapping>
