        <servlet-name>partialMatch</servlet-name>
        <servlet-class>servlets.partialMatch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>numberMatch</servlet-name>
        <servlet-class>servlets.numberMatch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addContacts</servlet-name>
        <servlet-class>servlets.addContacts</servlet-class>
    </servlet>
